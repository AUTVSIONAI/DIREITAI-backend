{
  "name": "direitaai-backend",
  "private": true,
  "version": "1.0.0",
  "description": "Backend da plataforma DireitaAI - API para conectar conservadores atravÃ©s de eventos, IA e comunidade",
  "author": "Equipe DireitaAI",
  "license": "MIT",
  "homepage": "https://direitai.com",
  "repository": {
    "type": "git",
    "url": "https://github.com/AUTVSIONAI/DIREITAI-backend.git"
  },
  "bugs": {
    "url": "https://github.com/AUTVSIONAI/DIREITAI-backend/issues"
  },
  "keywords": [
    "nodejs",
    "express",
    "api",
    "backend",
    "conservative",
    "events",
    "ai",
    "community",
    "supabase",
    "stripe"
  ],
  "main": "server.js",
  "scripts": {
    "start": "node server.js",
    "dev": "node server.js",
    "build": "echo 'No build step required for Node.js backend'",
    "test": "echo 'No tests specified'"
  },
  "dependencies": {
    "@supabase/supabase-js": "^2.35.0",
    "axios": "^1.6.7",
    "bcryptjs": "3.0.2",
    "cors": "^2.8.5",
    "dotenv": "^16.3.1",
    "express": "^4.18.2",
    "form-data": "4.0.4",
    "jsonwebtoken": "9.0.2",
    "multer": "2.0.2",
    "node-fetch": "3.3.2",
    "stripe": "18.4.0",
    "uuid": "^9.0.1"
  },
  "devDependencies": {
    "nodemon": "^3.0.1"
  },
  "engines": {
    "node": "18.x",
    "npm": ">=9.0.0"
  },
  "vercel": {
    "version": 2,
    "builds": [
      {
        "src": "server.js",
        "use": "@vercel/node"
      },
      {
        "src": "api/**/*.js",
        "use": "@vercel/node"
      }
    ],
    "routes": [
      {
        "src": "/api/(.*)",
        "dest": "/api/$1"
      },
      {
        "src": "/(.*)",
        "dest": "/server.js"
      }
    ]
  }
}